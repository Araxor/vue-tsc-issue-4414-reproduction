{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "module": "ESNext",
    "lib": ["ESNext", "DOM"],
    "moduleResolution": "Node",
    "strict": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "noEmit": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "skipLibCheck": true,
    "types": ["vuetify"]
  },
  "vueCompilerOptions": {
    "target": 2.7,
    "experimentalModelPropName": {
      "input-value": {
        "v-checkbox": true
      }
    }
  },
  "include": ["src"]
}
